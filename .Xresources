*foreground: #ff1100
*foreground_bold: #033f40
*cursor: #ffffff
URxvt*background: rgba:0500/0500/0500/f800
*color0: #000
*color9: #7600ff
!! red
*color1: #ff3311
*color8: #ff5511
!! purple
*color2: #ff5500
*color10:#501fb0 
!! yellow 
*color3: #f5bb00
*color11: #01a07f
!! dark purple
*color4: #606060
*color12: #a01100
!! background on autocomplete:
*color5: rgba:fa00/0a00/0000/f900
*color13: #ff00ff
!! green
*color6: #ebee00
*color14: #e1ee00
!! white
*color7: #38313f
*color15: #eeeeee
*color17: #ae0000
*color233: rgba:0500/0500/0500/f800
!!URxvt*.letterSpacing: 
!!URxvt.borderLess: 1
URxvt.intensityStyles:	false  
URxvt.tintColor:    #5448ff
URxvt*saveLines: 0
URxvt*.depth:		32
URxvt*.termName:  screen-256color
!!URxvt.font: xft:Droid Sans Mono Slashed for Powerline:antialias:size=11
URxvt.font: xft:DroidSansMono Nerd Font:antialias:size=10
!!URxvt.font: xft:DejaVu Sans Mono Nerd Font Complete:antialias:hinting=true:size=9.5
URxvt.scrollBar:	false
URxvt.cursorColor:	#406
URxvt*dynamicColors: false
!!URxvt.perl-ext-common:	default,matcher
URxvt.perl-lib:		$HOME/.config/Scripts
URxvt.url-launcher:	/usr/bin/xdg-open
URxvt.matcher.button:	1
URxvt.geometry: 101x42
URxvt.iconFile: /usr/share/icons/Mint-X/apps/96/gnome-eterm.svg
!!rofi.color-enabled:	true
URxvt.perl-ext-common: default,matcher,font-size
URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease
URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal
!!URxvt.keysym.C-equal:  perl:font-size:reset
!!URxvt.keysym.C-slash:  perl:font-size:show
URxvt.iso14755:        false
URxvt.iso14755_52:     false
URxvt.keysym.C-M-c:    builtin-string:
URxvt.keysym.C-M-v:    builtin-string:
URxvt.keysym.C-S-0x43: eval:selection_to_clipboard
URxvt.keysym.C-S-0x56: eval:paste_clipboard
URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~
URxvt.keysym.C-4: command:\033]11;rgba:0500/0500/0500/4800\007
URxvt.keysym.C-5: command:\033]11;rgba:0500/0500/0500/8800\007
URxvt.keysym.C-6: command:\033]11;rgba:0500/0500/0500/a800\007
URxvt.keysym.C-7: command:\033]11;rgba:0500/0500/0500/c800\007
URxvt.keysym.C-8: command:\033]11;rgba:0500/0500/0500/f800\007
URxvt.keysym.C-9: command:\033]11;#000000\007
URxvt.keysym.C-minus:  perl:font-size:show
